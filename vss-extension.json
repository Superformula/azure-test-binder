{
  "manifestVersion": 1,
  "id": "azure-test-binder",
  "name": "Azure Test Binder",
  "version": "0.3.0",
  "publisher": "Superformula",
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "description": "Associate JS/TS tests to work item ids in the test name.",
  "categories": ["Azure Pipelines"],
  "icons": {
    "default": "images/extension-icon.png"
  },
  "galleryFlags": ["Preview", "Public"],
  "files": [
    {
      "path": "task"
    }
  ],
  "content": {
    "details": {
      "path": "overview.md"
    },
    "license": {
      "path": "LICENSE"
    }
  },
  "contributions": [
    {
      "id": "azure-test-binder-task",
      "type": "ms.vss-distributed-task.task",
      "targets": ["ms.vss-distributed-task.tasks"],
      "properties": {
        "name": "task"
      }
    }
  ],
  "links": {
    "repository": {
      "uri": "https://github.com/Superformula/azure-test-binder"
    },
    "issues": {
      "uri": "https://github.com/Superformula/azure-test-binder/issues"
    }
  },
  "repository": {
    "type": "git",
    "uri": "https://github.com/Superformula/azure-test-binder"
  }
}
