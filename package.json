{
  "name": "azure-test-binder",
  "version": "0.2.6",
  "description": "Associate Azure Devops Work Items To Automated tests",
  "license": "MIT",
  "author": "James Dunnam",
  "scripts": {
    "prebuild": "yarn clean-generated",
    "build": "tsc -p tsconfig.release.json",
    "build-az-task": "npx tfx-cli extension create --manifest-globs vss-extension.json",
    "prebuild:ci": "run-s clean",
    "build:ci": "tsc -p tsconfig.release.json",
    "postbuild:ci": "cp package.json task/ && cp yarn.lock task/ && cp overview.md task/ && cp task.json task/ && cp -r images task/images/ && cd task && yarn install --production --frozen-lockfile",
    "build:watch": "tsc -w -p tsconfig.release.json",
    "checkcommit": "node ./scripts/commit-lint.js",
    "clean": "run-p clean-generated clean-logs",
    "clean-deps": "rimraf ./*/**/node_modules ./node_modules ",
    "clean-generated": "rimraf coverage tmp task *.vsix junit.xml",
    "clean-logs": "rimraf *.log",
    "commit": "git-cz",
    "format": "./scripts/format.sh && yarn run sort-pj",
    "lint": "scripts/check-format.sh",
    "manual-run": "NODE_ENV=local run-func task/index.js main",
    "nuke": "run-s clean clean-deps",
    "sort-pj": "npx sort-package-json",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn format && lint-staged",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "pre-push": "yarn checkcommit && pretty-quick --check"
    }
  },
  "lint-staged": {
    "*.{ts,js,md,yaml,yml,json,sh}": []
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "dependencies": {
    "azure-devops-node-api": "^10.1.1",
    "azure-pipelines-task-lib": "^3.0.1-preview",
    "inversify": "^5.0.1",
    "reflect-metadata": "^0.1.13",
    "tslib": "^2.0.1",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^11.0.0",
    "@types/faker": "^5.1.2",
    "@types/jest": "~26.0.10",
    "@types/mocha": "^8.0.3",
    "@types/node": "~14.11",
    "@types/q": "^1.5.4",
    "@typescript-eslint/eslint-plugin": "~4.0.0",
    "@typescript-eslint/parser": "~3.10.1",
    "commitizen": "^4.0.3",
    "conventional-changelog-cli": "^2.0.23",
    "cz-conventional-changelog": "^3.0.2",
    "cz-customizable": "^6.2.0",
    "dotenv": "^8.2.0",
    "eslint": "~7.9.0",
    "eslint-config-defaults": "^9.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.0.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-tsdoc": "^0.2.7",
    "eslint-plugin-unused-imports": "^0.1.3",
    "factory.ts": "^0.5.1",
    "faker": "^5.1.0",
    "husky": "^4.2.5",
    "jest": "~26.4.2",
    "jest-junit": "^11.1.0",
    "jest-mock-extended": "^1.0.10",
    "lint-staged": "^10.2.11",
    "mocha": "^8.1.3",
    "npm-run-all": "^4.1.5",
    "prettier": "~2.1.2",
    "pretty-quick": "^3.0.2",
    "rimraf": "~3.0.2",
    "run-func": "^1.0.5",
    "satisfied": "^1.1.2",
    "sort-package-json": "^1.44.0",
    "sync-request": "^6.1.0",
    "syncpack": "^5.6.10",
    "ts-jest": "~26.4.0",
    "tsutils": "~3.17.0",
    "typescript": "~4.0.2"
  },
  "engines": {
    "node": ">= 12.13 <13"
  },
  "volta": {
    "node": "12.18.3",
    "npm": "6.14.5",
    "yarn": "1.22.5"
  }
}
